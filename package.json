{
  "name": "AGENT-X",
  "version": "0.1.0",
  "description": "Agent Everything - Multi-agent AI CLI system with NVIDIA GPT-OSS-20B integration",
  "main": "./bin/AGENT-X.js",
  "type": "module",
  "scripts": {
    "agentx": "python -m cli.main",
    "start": "node bin/AGENT-X.js",
    "dev": "nodemon bin/AGENT-X.js",
    "menu": "node bin/AGENT-X.js menu",
    "setup": "node src/commands/setup-task.js",
    "chat": "node src/commands/chat.js",
    "response": "node src/commands/response.js",
    "agent": "node src/core/orchestrators/agent-orchestrator.js",
    "expert": "node src/agents/expert/hyper-expert-orchestrator.js",
    "update-knowledge": "node src/knowledge/knowledge-updater.js",
    "index": "node src/commands/index-codebase.js",
    "ui": "node src/commands/ink-interface.jsx",
    "test": "node ./tests/test-comprehensive.js",
    "test-basic": "node ./tests/test-agent-system.js",
    "test-expert": "node ./tests/test-expert-system.js",
    "test-all": "npm run test-basic && npm run test-expert && npm run test",
    "ink": "node bin/AGENT-X.js"
  },
  "keywords": [
    "nvidia",
    "gpt",
    "cli",
    "chat",
    "api"
  ],
  "bin": {
    "AGENT-X": "./bin/AGENT-X.js",
    "AGENT-X-chat": "./src/commands/chat.js",
    "AGENT-X-response": "./src/commands/response.js",
    "AGENT-X-setup": "./src/commands/setup-task.js"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "dotenv": "^16.4.5",
    "i18ntk": "^1.9.0",
    "ink": "^4.4.1",
    "inquirer": "^9.2.14",
    "openai": "^4.53.0",
    "ora": "^8.1.0",
    "react": "^18.2.0"
  }
}
