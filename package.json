{
  "name": "agent-x",
  "version": "0.1.0",
  "description": "Agent Everything - Multi-agent AI CLI system with NVIDIA GPT-OSS-20B integration",
  "main": "./bin/agentx.js",
  "type": "module",
  "scripts": {
    "agentx": "python agentx.py",
    "start": "python agentx.py",
    "menu": "python agentx.py",
