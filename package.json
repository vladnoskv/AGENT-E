    "ink": "^4.4.1",
    "inquirer": "^9.2.14",
    "openai": "^4.53.0",
    "ora": "^8.1.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/node": "^7.28.0",
    "@babel/plugin-transform-modules-commonjs": "^7.24.0",
